<state_machine>
    <transition from = "manual" to="autonomous" name="system_bringup"/>
    <transition from = "manual" to="emergency" name="fatal_error"/>

    <transition from = "autonomous" to="manual" name="system_bringup"/>
    <transition from = "autonomous" to="emergency" name="fatal_error"/>

    <transition from = "emergency" to="manual" name="manual_reset"/>
</state_machine>