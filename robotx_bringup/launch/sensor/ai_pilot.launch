<?xml version="1.0"?>
<launch>
    <group ns="ai_pilot_velodyne">
        <node pkg="nodelet" type="nodelet" name="ai_pilot_velodyne_manager" args="manager" />
        <node pkg="nodelet" type="nodelet" name="ai_pilot_velodyne_manager_driver"
                args="load velodyne_driver/DriverNodelet ai_pilot_velodyne_manager" >
            <param name="device_ip" value="192.168.1.201" />
            <param name="frame_id" value="ai_pilot_velodyne" />
            <param name="model" value="VLP16" />
            <param name="pcap" value="" />
            <param name="port" value="2368" />
            <param name="read_fast" value="false" />
            <param name="read_once" value="false" />
            <param name="repeat_delay" value="0.0" />
            <param name="rpm" value="600.0" />
        </node>
        <node pkg="nodelet" type="nodelet" name="ai_pilot_velodyne_manager_cloud"
                args="load velodyne_pointcloud/CloudNodelet ai_pilot_velodyne_manager">
            <param name="calibration" value="$(find velodyne_pointcloud)/params/VLP16db.yaml" />
            <param name="max_range" value="130.0" />
            <param name="min_range" value="1.0" />
        </node>
    </group>
</launch>